<!--pages/derive/derive.wxml-->
<view class="header">
  <view class="left">
    <view>导出时间:<text>{{time}}</text></view>
    <!-- <view>统计人数:<text>{{num}}</text></view> -->
    <view>execl下载地址:</view>
  </view>
  <view class="right" bindtap='refesh'><image mode="aspectFit" src="../../images/fs.png"></image></view>
</view>
<view class="copy">
    <text selectable='true'>{{url}}</text>
</view>
<button bindtap='copy'>复制在浏览器打开</button>
<button class="ray" bindtap='open'>直接打开</button>
<view class="header sx">
  <view class="left">
    <view>word下载地址:</view>
  </view>
</view>
<view class="copy">
    <text selectable='true'>{{url2}}</text>
</view>
<button bindtap='copy2'>复制在浏览器打开</button>
<button class="ray" bindtap='open2'>直接打开</button>